    "documittu-analyzer-ts": "^0.3.4",
    "documittu-template-default": "^0.3.5",
    "glamor": "^2.20.40",
    "prettier": "^1.10.2",
    "promise-land": "^0.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "tscomp": "0.15.3",
    "tslint": "^5.9.1",
    "tslint-config-pajn": "^1.0.0",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {
    "glamor": "^2.20.40",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "resolutions": {
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/recompose": "0.26.1"
  },
  "tscomp": {
    "mode": "lib"
  },
  "scripts": {
    "build": "tscomp build",
    "test": "tscomp test",
    "eject": "tscomp eject",
    "watch": "tscomp watch",
    "prepack": "tscomp build",
    "lint": "tslint",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "docs": "yarn docs-analyze && documittu build",
    "docs-dev": "documittu start",
    "docs-analyze": "documittu-analyzer-ts . > docs/analyze-result.json"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "(Apache-2.0 OR MIT)"
