    "@types/react-dom": "^16.0.4",
    "@types/recompose": "^0.26.1",
    "@types/webpack-env": "^1.13.6",
    "deep-equal": "^1.0.1",
    "glamorous": "^4.11.4",
    "history": "^4.7.2",
    "material-definitions": "^0.3.3",
    "react-router": "^4.2.0",
    "react-with-media": "^0.4.2",
    "recompose": "^0.27.0",
    "style-definitions": "^0.1.7"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react-router-dom": "^4.2.3",
