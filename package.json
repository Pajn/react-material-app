{
  "name": "react-material-app",
  "description": "High level material design React components",
  "homepage": "https://pajn.github.io/react-material-app/",
  "version": "0.6.0-rc.3",
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.2",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/recompose": "^0.30.6",
    "@types/webpack-env": "^1.13.9",
    "deep-equal": "^1.0.1",
    "history": "^4.9.0",
    "loadable-components": "^2.2.3",
    "material-definitions": "^0.3.3",
    "react-router": "^5.0.0",
    "react-with-media": "^1.1.3",
    "recompose": "^0.30.0",
    "style-definitions": "^0.1.7"
  },
  "devDependencies": {
    "@storybook/addon-info": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/jest": "^23.3.11",
    "@types/jest-image-snapshot": "^2.8.0",
    "@types/puppeteer": "^1.12.3",
    "@types/react-router-dom": "^4.3.3",
    "@types/storybook__react": "^4.0.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.1",
    "jest-image-snapshot": "^2.8.1",
    "jest-puppeteer": "^4.2.0",
    "prettier": "^1.17.1",
    "promise-land": "^0.1.2",
    "puppeteer": "^1.17.0",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "start-server-and-test": "^1.9.0",
    "tscomp-scripts": "1.0.0-rc.2",
    "tslint": "^5.17.0",
    "tslint-config-pajn": "^1.0.0",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": ">4.2.2 <6.0.0"
  },
  "resolutions": {
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/recompose": "0.30.6"
  },
  "tscomp": {
    "mode": "lib"
  },
  "scripts": {
    "build": "tscomp build",
    "test": "tscomp test",
    "eject": "tscomp eject",
    "watch": "tscomp watch",
    "prepack": "tscomp build",
    "lint": "tslint",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "docs": "yarn docs-analyze && documittu build",
    "docs-dev": "documittu start",
    "docs-analyze": "documittu-analyzer-ts . > docs/analyze-result.json",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:ci": "yarn storybook --ci",
    "build-storybook": "build-storybook -s public",
    "test:visual": "start-server-and-test storybook:ci http://localhost:9009 test:visual:test",
    "test:visual:test": "[ -z \"$IS_DOCKER\" ] && yarn test:visual:test-docker || yarn test:visual:test-raw",
    "test:visual:test-docker": "docker run -it --rm --cap-add=SYS_ADMIN -u `id -u` -w /home/pptruser/app -v \"$PWD\":/home/pptruser/app --net=host pieorpaj/node-puppeteer yarn test:visual:test-raw",
    "test:visual:test-raw": "jest --config ./jest.visual.config.json"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "(Apache-2.0 OR MIT)"
}
